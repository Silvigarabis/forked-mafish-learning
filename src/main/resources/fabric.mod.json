{
   "schemaVersion": 1,
   "id": "${fabricmod_id}",
   "version": "${version}",
   "name": "马夫鱼の奇怪模组",
   "description": "这是马夫鱼学习我的世界模组开发的时候构建的一些奇怪玩意，模组基于kuapenjoe的实例模组。",
   "authors": [
      "马夫鱼33",
      "Silvigarabis"
   ],
   "contact": {
      "homepage": "https://modrinth.com/mod/mafish-learning",
      "sources": "https://github.com/Silvigarabis/forked-mafish-learning"
   },
   "license": "MIT",
   "icon": "assets/${fabricmod_id}/icon.png",
   "environment": "*",
   "entrypoints": {
      "main": [
         "net.jiang.tutorialmod.TutorialMod"
      ],
      "client": [
         "net.jiang.tutorialmod.TutorialModClient"
      ],
      "fabric-datagen": [
         "net.jiang.tutorialmod.TutorialModDataGenerator"
      ]
   },
   "mixins": [
      "${fabricmod_id}.mixins.json"
   ],
   "depends": {
      "fabricloader": ">=0.15.1",
